#!/bin/bash
##
#SBATCH --job-name=lev2_analysis
#SBATCH --output=/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/log/lev2_analysis.out
#SBATCH --error=/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/log/lev2_analysis.err
#SBATCH --time=2:00:00
#SBATCH -p russpold,hns,normal
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=16
#SBATCH --mem-per-cpu=8G
# Outputs ---------------------------------
#SBATCH --mail-user=poldracklab@stanford.edu
#SBATCH --mail-type=END
# ------------------------------------------

. ${HOME}/.bashrc  
conda activate fmri_analysis

echo stopSignal:go
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:go:rt_centered one_sampt
echo stopSignal:response_time
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:response_time:rt_centered one_sampt
echo stopSignal:stop_failure-go
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:stop_failure-go:rt_centered one_sampt
echo stopSignal:stop_failure-stop_success
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:stop_failure-stop_success:rt_centered one_sampt
echo stopSignal:stop_failure
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:stop_failure:rt_centered one_sampt
echo stopSignal:stop_success-go
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:stop_success-go:rt_centered one_sampt
echo stopSignal:stop_success-stop_failure
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:stop_success-stop_failure:rt_centered one_sampt
echo stopSignal:stop_success
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:stop_success:rt_centered one_sampt
echo stopSignal:task
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stopSignal:task:rt_centered one_sampt
echo stroop:response_time
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stroop:response_time:rt_centered one_sampt
echo stroop:stroop_cong_v_baseline
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stroop:stroop_cong_v_baseline:rt_centered one_sampt
echo stroop:stroop_incong_minus_cong
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stroop:stroop_incong_minus_cong:rt_centered one_sampt
echo stroop:stroop_incong_v_baseline
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stroop:stroop_incong_v_baseline:rt_centered one_sampt
echo stroop:task
/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/analyze_lev2_SJ.py stroop:task:rt_centered one_sampt
